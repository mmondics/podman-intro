function getSearchURL(){state_sel=$("#form-park").val();activity_sel_array=$("#form-activity").val();topic_sel_array=$("#form-topic").val();urlString="";activity_list="";topic_list="";if(state_sel>"")urlString="s\x3d"+state_sel;if(activity_sel_array>"")for(i$jscomp$0=0;i$jscomp$0<activity_sel_array.length;i$jscomp$0++){if(activity_list=="")activity_list=activity_sel_array[i$jscomp$0];else activity_list+=","+activity_sel_array[i$jscomp$0];if(urlString>"")urlString+="\x26";urlString+="a\x3d"+activity_list}if(topic_sel_array>
"")for(i$jscomp$0=0;i$jscomp$0<topic_sel_array.length;i$jscomp$0++){if(topic_list=="")topic_list=topic_sel_array[i$jscomp$0];else topic_list+=","+topic_sel_array[i$jscomp$0];if(urlString>"")urlString+="\x26";urlString+="t\x3d"+topic_list}urlString+="\x26p\x3d"+current_page;urlString+="\x26v\x3d"+view_all;if(urlString>"")rewriteURL("/findapark/advanced-search.htm?"+urlString);else rewriteURL("/findapark/advanced-search.htm");searchParks()}function displaySearchResults(){total_results=search_results.length;
