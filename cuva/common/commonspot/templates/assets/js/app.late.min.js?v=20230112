Schedule.prototype.getActiveExceptions=function(date){return this.exceptions.filter(function(exception){return exception.isActive(date)})};Schedule.prototype.getHours=function(date){var givenDate=date instanceof Date?new Date(date.valueOf()):new Date;var givenDay=givenDate.getDay();var activeExceptions=this.getActiveExceptions(givenDate);if(activeExceptions.length>0){var activeException=activeExceptions.pop();return activeException.hours[givenDay]}return this.hours[givenDay]};ScheduleException.prototype.getStartDate=
function(startYear){var year=startYear||(new Date).getFullYear();if(this.holidayStart)return getHolidayDate(this.holidayStart,year);else{var startMonth=this.dateStart.split(" ")[0];var startDay=parseInt(this.dateStart.split(" ")[1]);return new Date(year,monthNames.indexOf(startMonth),startDay,0,0,0,0)}};ScheduleException.prototype.getEndDate=function(startYear){var year=startYear||(new Date).getFullYear();var startDate=this.getStartDate(year);var sameYearEnd;if(this.holidayEnd){var endDate=getHolidayDate(this.holidayEnd,
year);endDate.setHours(23,59,59,999)}else{var endMonth=this.dateEnd.split(" ")[0];var endDay=parseInt(this.dateEnd.split(" ")[1]);endDate=new Date(year,monthNames.indexOf(endMonth),endDay,23,59,59,999)}if(endDate<startDate)if(this.holidayEnd){endDate=getHolidayDate(this.holidayEnd,year+1);endDate.setHours(23,59,59,999)}else endDate.setFullYear(year+1);return endDate};ScheduleException.prototype.getDateRange=function(startYear){if(startYear===undefined){var today=new Date;var currentYear=today.getFullYear();
if(today<this.getStartDate(currentYear))if(today>this.getEndDate(currentYear-1))startYear=currentYear;else startYear=currentYear-1;else if(today<=this.getEndDate(currentYear))startYear=currentYear;else startYear=currentYear+1}var startDate=this.getStartDate(startYear);var startDateString=monthNames[startDate.getMonth()]+" "+startDate.getDate();var endDate=this.getEndDate(startYear);var endDateString=monthNames[endDate.getMonth()]+" "+endDate.getDate();return startDateString+"\u2013"+endDateString};
ScheduleException.prototype.isActive=function(date){var givenDate=date instanceof Date?new Date(date.valueOf()):new Date;if(givenDate<this.getStartDate(givenDate.getFullYear()))if(givenDate>=this.getStartDate(givenDate.getFullYear()-1)&&givenDate<=this.getEndDate(givenDate.getFullYear()-1))return true;else return false;else return givenDate<=this.getEndDate(givenDate.getFullYear())};HolidayScheduleException.prototype=Object.create(ScheduleException.prototype);HolidayScheduleException.prototype.constructor=
